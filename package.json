{
  "name": "quiz-app",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node server/start.js",
    "start:dev": "nodemon server/start.js",
    "webpack-watch": "webpack --config ./webpack.sohil.config.js --progress --colors --watch",
    "dev": "node ./src/utils/dev-server.js",
    "build": "webpack --config ./webpack.production.config.js --colors --progress",
    "lint": "node_modules/eslint/bin/eslint.js 'src/**/*.@(js|jsx)' 'server/**/*.@(js|jsx)' 'test/**/*.@(js|jsx)'",
    "postgres": "postgres -D /usr/local/var/postgres",
    "heroku:production:postgres": "heroku pg:psql --app cul-app",
    "heroku:dev:postgres": "heroku pg:psql --app cul-app HEROKU_POSTGRESQL_SILVER",
    "load-schema": "heroku pg:psql --app cul-app DATABASE < ./server/lib/schema.txt",
    "coverage": "./node_modules/.bin/babel-node ./node_modules/babel-istanbul/lib/cli.js cover ./node_modules/tape/bin/tape --require ./test/utils/jsdom.js 'test/**/*.@(js|jsx)'",
    "check-coverage": "babel-node ./node_modules/babel-istanbul/lib/cli.js check-coverage",
    "pretest": "node ./test/utils/init.js",
    "test": "tape -r babel-register --require ./test/utils/jsdom.js 'test/**/*.@(js|jsx)' | tap-spec",
    "postinstall": "webpack --config ./webpack.production.config.js --progress --colors",
    "make-docs": "./node_modules/.bin/jsdoc -c ./conf.json -r"
  },
  "repository": {
    "type": "git",
    "url": "git+https://sohilpandya@github.com/cul-2016/quiz.git"
  },
  "author": "sohilpandya, minaorangina",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/cul-2016/quiz/issues"
  },
  "homepage": "https://github.com/cul-2016/quiz#readme",
  "config": {
    "ghooks": {
      "pre-commit": "npm run test && npm run lint",
      "post-merge": "npm install"
    }
  },
  "dependencies": {
    "axios": "^0.12.0",
    "babel-cli": "^6.10.1",
    "babel-eslint": "^6.1.0",
    "babel-istanbul": "^0.11.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.9.0",
    "bcrypt": "^0.8.7",
    "bell": "^7.9.1",
    "classnames": "^2.2.5",
    "css-loader": "^0.23.1",
    "eslint": "^3.0.1",
    "file-loader": "^0.9.0",
    "hapi": "^13.5.0",
    "hoek": "^4.0.1",
    "inert": "^4.0.1",
    "istanbul": "^1.0.0-alpha.2",
    "json-loader": "^0.5.4",
    "lodash": "^4.15.0",
    "node-sass": "^3.8.0",
    "pg": "^6.0.1",
    "postcss-loader": "^0.9.1",
    "react": "^15.3.0",
    "react-addons-css-transition-group": "^15.3.0",
    "react-addons-test-utils": "^15.2.0",
    "react-addons-update": "^15.2.1",
    "react-dom": "^15.2.0",
    "react-hot-loader": "^1.3.0",
    "react-motion": "^0.4.4",
    "react-redux": "^4.4.5",
    "react-router": "^2.5.2",
    "redux": "^3.5.2",
    "redux-thunk": "^2.1.0",
    "sass-loader": "^4.0.0",
    "simple-statistics": "^2.1.0",
    "socket.io": "^1.4.8",
    "socket.io-client": "^1.4.8",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1"
  },
  "devDependencies": {
    "eslint-plugin-react": "^5.2.2",
    "ghooks": "^1.3.2",
    "jsdoc": "^3.4.0",
    "jsdom": "^9.4.1",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.0",
    "webpack-dev-server": "^1.14.1"
  }
}
