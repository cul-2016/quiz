{
  "name": "quiz-app",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node server/start.js",
    "start:dev": "nodemon server/start.js",
    "webpack-watch": "webpack --config ./webpack.watch.config.js --progress --colors --watch",
    "dev": "node ./src/utils/dev-server.js",
    "build": "webpack --config ./webpack.production.config.js --colors --progress",
    "lint": "node_modules/eslint/bin/eslint.js 'src/**/*.js' 'server/**/*.js' 'test/**/*.js' --fix",
    "postgres": "postgres -D /usr/local/var/postgres",
    "heroku:production:postgres": "heroku pg:psql --app cul-app",
    "heroku:staging:postgres": "heroku pg:psql --app cul-app-staging Database",
    "load-staging-schema": "heroku pg:psql --app cul-app-staging Database < ./test/utils/test-schema-local.txt",
    "coverage": "export TESTING=true && ./node_modules/.bin/babel-node ./node_modules/babel-istanbul/lib/cli.js cover tape --require ./test/utils/jsdom.js 'test/runner.js'",
    "check-coverage": "./node_modules/.bin/babel-node ./node_modules/babel-istanbul/lib/cli.js check-coverage",
    "test": "export TESTING=true && node_modules/.bin/tape -r babel-register --require ./test/utils/jsdom.js 'test/runner.js' | tap-spec",
    "test:front": "export TESTING=true && node_modules/.bin/tape -r babel-register --require ./test/utils/jsdom.js 'test/front/runner.js' | tap-spec",
    "test:front:watch": "nodemon -w src -w test/front -x 'npm run test:front'",
    "test:back": "export TESTING=true && node_modules/.bin/tape test/back/runner.js | tap-spec",
    "test:back:watch": "nodemon -w server -w test/back -x 'npm run test:back'",
    "migrations": "node callMigrations",
    "postinstall": "webpack --config ./webpack.production.config.js --progress --colors && npm run migrations"
  },
  "engines": {
    "node": "6.11.1",
    "npm": "5.6.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://sohilpandya@github.com/cul-2016/quiz.git"
  },
  "author": "sohilpandya, minaorangina",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/cul-2016/quiz/issues"
  },
  "homepage": "https://github.com/cul-2016/quiz#readme",
  "config": {
    "ghooks": {
      "pre-commit": "npm run test"
    }
  },
  "dependencies": {
    "async": "^2.5.0",
    "axios": "^0.15.3",
